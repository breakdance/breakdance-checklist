{
  "name": "breakdance-checklist",
  "description": "Plugin that adds checklist rendering support to breakdance, similar to task lists in github-flavored-markdown.",
  "version": "0.1.2",
  "homepage": "https://github.com/breakdance/breakdance-checklist",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "breakdance/breakdance-checklist",
  "bugs": {
    "url": "https://github.com/breakdance/breakdance-checklist/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.7"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "get-value": "^2.0.6"
  },
  "devDependencies": {
    "breakdance": "^0.1.7",
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "breakdance",
    "breakdanceplugin",
    "checklist",
    "compile",
    "compiler",
    "convert",
    "html",
    "markdown",
    "parse",
    "parser",
    "render",
    "transform"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "breakdance-util",
        "breakdance-reflinks"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}